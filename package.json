{
  "name": "react-native-circular-progress-indicator",
  "version": "4.4.2",
  "description": "React Native customizable circular progress indicator",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "files": [
    "src",
    "lib",
    "!**/__tests__"
  ],
  "scripts": {
    "test": "jest --forceExit",
    "lint": "eslint --ext .ts,.tsx . --max-warnings 0",
    "lint:fix": "eslint --ext .ts,.tsx . --fix",
    "tsc": "tsc",
    "ci": "npm run lint && npm run tsc",
    "build": "bob build",
    "prepare": "npm run build && husky install"
  },
  "repository": {
    "name": "GitHub",
    "type": "git",
    "url": "git+https://github.com/HarryMtv/react-native-circular-progress-indicator.git"
  },
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-native": ">=0.82.1",
    "react-native-reanimated": ">=3.16.0",
    "react-native-svg": ">=15.0.0"
  },
  "author": "Igor Martynov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HarryMtv/react-native-circular-progress-indicator/issues"
  },
  "homepage": "https://github.com/HarryMtv/react-native-circular-progress-indicator.git",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.0",
    "@babel/runtime": "^7.26.0",
    "@eslint/js": "^9.39.1",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/babel-preset": "^0.82.0",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.12",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.12.1",
    "@typescript-eslint/parser": "^8.12.1",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^8.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-native": "0.82.1",
    "react-native-builder-bob": "^0.40.15",
    "react-native-gesture-handler": "^2.17.1",
    "react-native-reanimated": "^4.1.5",
    "react-native-svg": "^15.3.0",
    "react-test-renderer": "^19.0.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.47.0"
  },
  "keywords": [
    "react-native",
    "react",
    "native",
    "animated",
    "reanimated",
    "chart",
    "ui",
    "animated-chart",
    "progress-indicator",
    "animated-progress-indicator",
    "animated-progress-bar",
    "circular-progress-bar",
    "circular-progress",
    "circular-progress-indicator",
    "mobile",
    "android",
    "ios"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironment": "node",
    "clearMocks": true,
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ],
    "transformIgnorePatterns": []
  },
  "dependencies": {
    "react-native-redash": "^18.1.0"
  }
}
